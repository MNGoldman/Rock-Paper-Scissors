d__scissors {\n    transform: rotate(170deg);\n    margin-right: 12vw;\n}\n\n.compHand__scissors {\n    transform: rotate(10deg);\n    margin-left: 12vw;\n}\n\n.bounce {\n    animation: bounce 2s;\n}\n\n@keyframes bounce {\n    0%,\n    25%,\n    50%,\n    75%,\n    100%,\n    \n    0% {\n        transform: translateY(0);\n    }\n    20% {\n        transform: translateY(-7vh);\n    }\n    40% {\n        transform: translateY(-7vh);\n    }\n\n    60% {\n        transform: translateY(-7vh);\n    }\n\n    80% {\n        transform: translateY(-7vh);\n    }\n    100% {\n        transform: translateY(0);\n    }\n}\n","css\\Partials\\_components.scss":".scoreboard {\n    display: flex;\n    justify-content: center;\n    align-content: center;\n    flex-direction: row;\n    font-size: 1rem;\n    margin: 5vh 0vh;\n}\n\n.newgame {\n    background: transparent;\n    border: 3px solid map-get($colors, \"GradientBlue\");\n    color: map-get($colors, \"GradientBlue\");\n    font-size: 0.8rem;\n    margin-bottom: 5vh;\n    padding: 0.5vh;\n    border-radius: 2%;\n }\n \n .newgame:hover {\n    background: map-get($colors, \"GradientBlue\");\n    border: 5px solid map-get($colors, \"GradientBlue\");\n    color: map-get($colors, \"Default\");\n    cursor: $link-cursor;\n }\n\n .newgame:focus {\n    background: map-get($colors, \"GradientBlue\");\n    border: 5px solid map-get($colors, \"GradientBlue\");\n    color: map-get($colors, \"Default\");\n}\n\n .title {\n    font-size: .85rem;\n    margin-top: 1vh;\n    flex-direction: column;\n}\n\n.rock {\n    color: map-get($colors, \"Rock\");\n    cursor: $link-cursor;\n}\n\n.rock:focus {\n    background: map-get($colors, \"GradientBlue\");\n    border: 5px solid map-get($colors, \"GradientBlue\");\n}\n\n.paper {\n    color: map-get($colors, \"Paper\");\n    cursor: $link-cursor;\n}\n\n.paper:focus {\n    background: map-get($colors, \"GradientBlue\");\n    border: 5px solid map-get($colors, \"GradientBlue\");\n}\n\n.scissors {\n    color: map-get($colors, \"Scissors\");\n    cursor: $link-cursor;\n}\n\n.scissors:focus {\n    background: map-get($colors, \"GradientBlue\");\n    border: 5px solid map-get($colors, \"GradientBlue\");\n}\n"},"lineCount":null}},"error":null,"hash":"8f0f801ee32d645d49f63458531593e2","cacheData":{"env":{}}}